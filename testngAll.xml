<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
  <parameter name="browser" value="chrome"/>
    <test verbose="2" preserve-order="false" name="D:/FinalProjectAutomation">
        <classes>
            <class name="tests.TestPlacingAnOrderUsingSubcategory">
                <methods>
                    <include name="testBillingInformationFeature"/>
                </methods>
            </class>
            <class name="tests.TestContactUsPage">
                <methods>
                    <include name="testContactUsfeature"/>
                    <include name="testContactUsfeatureWithInvalidMassage"/>
                    <include name="testContactUsfeatureWithInvalidEmail"/>
                </methods>
            </class>
            <class name="tests.TestForgotPasswordPage">
                <methods>
                    <include name="testForgotPassField"/>
                    <include name="testForgotPassFieldWithInvalidEmail"/>
                </methods>
            </class>
            <class name="tests.TestLoginToCreateOrder">
                <methods>
                    <include name="testLoginFeature"/>
                </methods>
            </class>
            <class name="tests.TestRegistrationPage">
                <methods>
                    <include name="testValidRegistration"/>
                </methods>
            </class>
            <class name="tests.TestSearchPage">
                <methods>
                    <include name="testSearchFeature"/>
                </methods>
            </class>
            <class name="tests.TestSuggestionSearch">
                <methods>
                    <include name="testSuggestionSearch"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>